<div class="maincontainer" id="petWallPage">
    <!--     Start of Nav Bar          -->
    <nav class="navbar navbar-default navbar-fixed-top">
        <div class="navigation">
            <div class="navigation_pages">
                <button type="button" class="btn btn-default" id="home" onclick='location.href="/";'>

                        <span class="glyphicon glyphicon-home
                        "aria-hidden="true"></span>
                        </button>
                <div class="sitename">Meet Your Muppet</div>
            </div>
        </div>
    </nav>
    <!--End of Nav Bar-->
    <div class="petwallContainer">
        <div class="masonry">
            {{#each pets}}

            <div class="item">
                <div class="thumbnail">
                    <img src="{{media.photos.photo.2.$t}}" />
                    <h2 class="name">{{ name.$t }}</h2>
                    <h2 class="contact">{{contact.email.$t}}</h2>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
